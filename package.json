{
  "name": "next-image-loader",
  "version": "0.0.0",
  "description": "Plugin to transparently override the loader in next/image.",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": "./build/index.js",
    "./image-loader": "./build/image-loader.js"
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:aiji42/next-image-loader"
  },
  "author": "aiji42 <aiji42@gmail.com> (https://twitter.com/aiji42_dev)",
  "license": "MIT",
  "keywords": [
    "next.js",
    "next",
    "react",
    "plugins",
    "image optimize",
    "image loader"
  ],
  "scripts": {
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "build": "node build.js",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationDir './build'",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "prepack": "yarn build"
  },
  "peerDependencies": {
    "next": ">=10.0.0",
    "react": ">=17.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/node": "^18.0.1",
    "@vitejs/plugin-react": "^1.3.2",
    "c8": "^7.11.3",
    "esbuild": "^0.14.48",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "next": "^12.2.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-release": "^19.0.3",
    "typescript": "^4.7.4",
    "vitest": "^0.17.0"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write"
    ]
  }
}
